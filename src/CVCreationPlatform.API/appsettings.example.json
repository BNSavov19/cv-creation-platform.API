{
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "<Issuer>",
    "Audience": "<Audience>",
    "Key": "<JWT secret key>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "<DB Connection String>"
  },
  "Azure": {
    "TextAnalytics": {
      "Endpoint": "<Text Analytics Endpoint>",
      "Key": "<Text Analytics Access Key>"
    },
    "CognitiveSearch": {
      "Key": "<Cognitive Search Access key>",
      "IndexName": "<Cognitive Search Index Name>",
      "ServiceName": "<Service Name>"
    },
    "EmailClient": {
      "ConnectionString": "<Azure Communication Services Email Client Connection String>"
    },
    "Storage": {
      "StorageConnectionString": "<Storage Account Connection String>",
      "ContainerName": "<Blob Storage Container Name>"
    }
  }
}
